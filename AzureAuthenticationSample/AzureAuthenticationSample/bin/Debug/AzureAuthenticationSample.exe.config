<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Configuration Needed for All Methods-->
    <add key="ClientId" value="00000000-0000-0000-0000-000000000000"/>          <!-- GUID for AAD application configured as Native Client App in AAD tenant specified above -->
    
    <!--Configuration Needed for Method 1 only-->
    <add key="ADALServiceURL" value="https://login.microsoftonline.com"/>       <!-- Service root URL for ADAL authentication service WITH NO TRAILING SLASH! -->
    <add key="ADALRedirectURL" value="https://localhost/"/>                     <!-- Redirect URL for ADAL authentication service MUST MATCH YOUR AAD APP CONFIGURATION! -->
    <add key="ARMBillingServiceURL" value="https://management.azure.com"/>      <!-- Service root URL for ARM/Billing service WITH NO TRAILING SLASH!  -->
    <add key="TenantDomain" value="ENTER.AZURE.AD.DNS.NAME"/>                   <!-- DNS name for your Azure AD tenant -->

    <!--Configuration Needed for Method 2 and 3 only-->
    <add key="TenantID" value="00000000-0000-0000-0000-000000000000"/>          <!-- GUID for AAD application configured as Native Client App in AAD tenant specified above -->

    <!--Configuration Needed for Method 2 only-->
    <add key="Password" value="ENTER.PASSWORD"/>                       <!-- Password - You can add encryption logic in your application and store the encrypted version here -->

    <!--Configuration Needed for Method 3 only-->
    <add key="CertificateName" value="ENTER.AZURE.AD.DNS.NAME"/>                <!-- Certificate Name in your local user store -->


  </appSettings>
</configuration>